  {
    "name": "Scout QA Highlighter",
    "short_name": "Scout",
    "description": "Locate data-test attributes on any webpage",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
      "32": "img/icon_32x32.png",
      "128": "img/icon_128x128.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "css": [
          "style.css"
        ]
      }
    ],
    "browser_action": {
      "default_icon": "img/icon_32x32.png",
      "default_popup": "popup.html"
    }
  }